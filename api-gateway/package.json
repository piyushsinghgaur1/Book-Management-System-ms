{
  "name": "api-gateway",
  "version": "0.0.1",
  "description": "api-gateway",
  "keywords": [
    "loopback-application",
    "loopback"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": "18 || 20 || 22"
  },
  "scripts": {
    "build": "lb-tsc",
    "build:watch": "lb-tsc --watch",
    "lint": "yarn run eslint && yarn run prettier:check",
    "lint:fix": "yarn run eslint:fix && yarn run prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "yarn run prettier:cli -l",
    "prettier:fix": "yarn run prettier:cli --write",
    "eslint": "lb-eslint --report-unused-disable-directives .",
    "eslint:fix": "yarn run eslint --fix",
    "pretest": "yarn run rebuild",
    "test": "lb-mocha --allow-console-logs \"dist/__tests__\"",
    "posttest": "yarn run lint",
    "test:dev": "lb-mocha --allow-console-logs dist/__tests__/**/*.js && yarn run posttest",
    "docker:build": "docker build -t api-gateway .",
    "docker:run": "docker run -p 3000:3000 -d api-gateway",
    "premigrate": "yarn run build",
    "migrate": "node ./dist/migrate",
    "preopenapi-spec": "yarn run build",
    "openapi-spec": "node ./dist/openapi-spec",
    "prestart": "yarn run rebuild",
    "start": "node -r newrelic dist/index.js",
    "clean": "lb-clean dist *.tsbuildinfo .eslintcache",
    "rebuild": "yarn run clean && yarn run build"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "piyushsinghgaur1 <piyush.singh@sourcefuse.com>",
  "license": "",
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "@loopback/authentication-jwt": "^0.15.12",
    "@loopback/boot": "^7.0.11",
    "@loopback/context": "^7.0.12",
    "@loopback/core": "^6.1.9",
    "@loopback/repository": "^7.0.11",
    "@loopback/rest": "^14.0.12",
    "@loopback/rest-explorer": "^7.0.11",
    "@loopback/service-proxy": "^7.0.11",
    "@types/passport-http-bearer": "^1.0.41",
    "@types/passport-local": "^1.0.38",
    "axios": "^1.8.4",
    "chai": "^5.2.0",
    "dotenv": "^16.4.7",
    "jsonwebtoken": "^9.0.2",
    "loopback4-authentication": "^12.2.0",
    "loopback4-authorization": "^7.0.3",
    "newrelic": "^12.16.1",
    "tslib": "^2.0.0"
  },
  "devDependencies": {
    "@loopback/build": "^11.0.10",
    "@loopback/eslint-config": "^15.0.5",
    "@loopback/testlab": "^7.0.11",
    "@types/axios": "^0.14.4",
    "@types/chai": "^5.2.1",
    "@types/jasmine": "^5.1.7",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "^16.18.126",
    "eslint": "^8.57.1",
    "jasmine": "^5.6.0",
    "sinon": "^20.0.0",
    "source-map-support": "^0.5.21",
    "ts-jest": "^29.3.1",
    "typescript": "~5.2.2"
  }
}
